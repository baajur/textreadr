    title = "{textreadr}: Read Text Documents into R",
    author = "Tyler W. Rinker",
    address = "Buffalo, New York",
    note = "version 1.0.3",
    year = "2020",
    url = "http://github.com/trinker/textreadr",
    textVersion  = paste("Rinker, T. W. (2020).",
        "textreadr: Read Text Documents into R",
        "version 1.0.3. Buffalo, New York.",
        "http://github.com/trinker/textreadr")
)
